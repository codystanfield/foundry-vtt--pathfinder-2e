ype":"String","value":"save"},"sustained":{"label":"Can be Sustained","type":"Boolean","value":false},"target":{"label":"Target","type":"String","value":""},"time":{"label":"Casting Time","type":"String","value":"2"},"traditions":{"label":"Spell Traditions","type":"String","value":["arcane","primal"]},"traits":{"label":"Spell Traits","rarity":{"value":"common"},"type":"String","value":["evocation","fire"]},"usage":{"value":"held-in-one-hand"}},"flags":{"_sheetTab":"details"},"img":"systems/pf2e/icons/spells/fireball.jpg","name":"Fireball","sort":700000,"type":"spell"},{"_id":"MkytvxuCurqRITL7","data":{"ability":{"label":"Spellcasting Ability","type":"String","value":""},"area":{"areaType":"","label":"Area","type":"String","value":null},"areasize":{"value":""},"components":{"label":"Spell Components","type":"String","value":"material, somatic, verbal"},"cost":{"label":"Cost","type":"String","value":""},"damage":{"applyMod":false,"label":"Spell Damage","type":"String","value":""},"damageType":{"label":"Damage Type","type":"String","value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p>You send the creature a mental message of 25 words or fewer, and it can respond immediately with its own message of 25 words or fewer.</p>"},"duration":{"label":"Duration","type":"String","value":""},"level":{"label":"Spell Level","type":"Number","value":5},"location":{"value":"nUR46YzW01Eu49R1"},"materials":{"label":"Materials","type":"String","value":""},"prepared":{"label":"Prepared Spell","type":"String","value":""},"primarycheck":{"value":""},"range":{"label":"Range","type":"String","value":"planetary"},"rarity":{"value":"common"},"save":{"basic":"","label":"Saving Throw","type":"String","value":""},"scaling":{"formula":"","mode":"none"},"school":{"label":"Spell School","type":"String","value":"div"},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"source":{"label":"Source","type":"String","value":"https://2e.aonprd.com/Spells.aspx?ID=272"},"spellCategorie":{"value":"spell"},"spellCategory":{"value":""},"spellType":{"label":"Spell Type","type":"String","value":"utility"},"sustained":{"label":"Can be Sustained","type":"Boolean","value":false},"target":{"label":"Target","type":"String","value":"1 creature with whom you are familiar"},"time":{"label":"Casting Time","type":"String","value":"3"},"traditions":{"label":"Spell Traditions","type":"String","value":["arcane","divine","occult"]},"traits":{"label":"Spell Traits","rarity":{"value":"common"},"type":"String","value":["divination","mental"]},"usage":{"value":"held-in-one-hand"}},"flags":{},"img":"systems/pf2e/icons/spells/sending.jpg","name":"Sending","sort":800000,"type":"spell"},{"_id":"xn5SLYC5nNBxRFkk","data":{"ability":{"label":"Spellcasting Ability","type":"String","value":""},"area":{"areaType":"","label":"Area","type":"String","value":null},"areasize":{"value":""},"components":{"label":"Spell Components","type":"String","value":"material, somatic, verbal"},"cost":{"label":"Cost","type":"String","value":""},"damage":{"applyMod":false,"label":"Spell Damage","type":"String","value":""},"damageType":{"label":"Damage Type","type":"String","value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p>You shape a wall of solid stone. You create a 1-inch-thick wall of stone up to 120 feet long, and 20 feet high. You can shape the wall's path, placing each 5 feet of the wall on the border between squares. The wall doesn't need to stand vertically, so you can use it to form a bridge or set of stairs, for example. You must conjure the wall in an unbroken open space so its edges don't pass through any creatures or objects, or the spell is lost. Each 5-foot-by-5-foot section of the wall has AC 10, Hardness 14, and 50 Hit Points, and it's immune to critical hits and precision damage. A destroyed section of the wall can be moved through, but the rubble created from it is difficult terrain.</p>\n<hr>\n<p><strong>Heightened (+2)</strong> The Hit Points of each section of the wall increase by 15.</p>"},"duration":{"label":"Duration","type":"String","value":""},"level":{"label":"Spell Level","type":"Number","value":5},"location":{"value":"nUR46YzW01Eu49R1"},"materials":{"label":"Materials","type":"String","value":""},"prepared":{"label":"Prepared Spell","type":"String","value":""},"primarycheck":{"value":""},"range":{"label":"Range","type":"String","value":"120 feet"},"rarity":{"value":"common"},"save":{"basic":"","label":"Saving Throw","type":"String","value":""},"scaling":{"formula":"","mode":"none"},"school":{"label":"Spell School","type":"String","value":"con"},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"source":{"label":"Source","type":"String","value":"https://2e.aonprd.com/Spells.aspx?ID=365"},"spellCategorie":{"value":"spell"},"spellCategory":{"value":""},"spellType":{"label":"Spell Type","type":"String","value":"utility"},"sustained":{"label":"Can be Sustained","type":"Boolean","value":false},"target":{"label":"Target","type":"String","value":""},"time":{"label":"Casting Time","type":"String","value":"3"},"traditions":{"label":"Spell Traditions","type":"String","value":["arcane","primal"]},"traits":{"label":"Spell Traits","rarity":{"value":"common"},"type":"String","value":["conjuration","earth"]},"usage":{"value":"held-in-one-hand"}},"flags":{},"img":"systems/pf2e/icons/spells/wall-of-stone.jpg","name":"Wall of Stone","sort":900000,"type":"spell"},{"_id":"ojsbvCKkxJHFzm1a","data":{"ability":{"label":"Spellcasting Ability","type":"String","value":""},"area":{"areaType":"","label":"Area","type":"String","value":null},"areasize":{"value":""},"components":{"label":"Spell Components","type":"String","value":"somatic, verbal"},"cost":{"label":"Cost","type":"String","value":""},"damage":{"applyMod":false,"label":"Spell Damage","type":"String","value":""},"damageType":{"label":"Damage Type","type":"String","value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p>Opening a door that bypasses normal space, you instantly transport yourself and any items you're wearing and holding from your current space to a clear space within range you can see. If this would bring another creature with you—even if you're carrying it in an extradimensional container—the spell is lost.</p>\n<hr>\n<p><strong>Heightened (5th)</strong> The range increases to 1 mile. You don't need to be able to see your destination, as long as you have been there in the past and know its relative location and distance from you. You are temporarily immune for 1 hour.</p>"},"duration":{"label":"Duration","type":"String","value":""},"level":{"label":"Spell Level","type":"Number","value":4},"location":{"value":"nUR46YzW01Eu49R1"},"materials":{"label":"Materials","type":"String","value":""},"prepared":{"label":"Prepared Spell","type":"String","value":""},"primarycheck":{"value":""},"range":{"label":"Range","type":"String","value":"120 feet"},"rarity":{"value":"common"},"save":{"basic":"","label":"Saving Throw","type":"String","value":""},"scaling":{"formula":"","mode":"none"},"school":{"label":"Spell School","type":"String","value":"con"},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"source":{"label":"Source","type":"String","value":"https://2e.aonprd.com/Spells.aspx?ID=69"},"spellCategorie":{"value":"spell"},"spellCategory":{"value":""},"spellType":{"label":"Spell Type","type":"String","value":"utility"},"sustained":{"label":"Can be Sustained","type":"Boolean","value":false},"target":{"label":"Target","type":"String","value":""},"time":{"label":"Casting Time","type":"String","value":"2"},"traditions":{"label":"Spell Traditions","type":"String","value":["arcane","occult"]},"traits":{"label":"Spell Traits","rarity":{"value":"common"},"type":"String","value":["conjuration","teleportation"]},"usage":{"value":"held-in-one-hand"}},"flags":{},"img":"systems/pf2e/icons/spells/dimension-door.jpg","name":"Dimension Door","sort":1000000,"type":"spell"},{"_id":"rK3ZWSvuaq681qSH","data":{"ability":{"label":"Spellcasting Ability","type":"String","value":""},"area":{"areaType":"","label":"Area","type":"String","value":null},"areasize":{"value":""},"components":{"label":"Spell Components","type":"String","value":"somatic, verbal"},"cost":{"label":"Cost","type":"String","value":""},"damage":{"applyMod":false,"label":"Spell Damage","type":"String","value":""},"damageType":{"label":"Damage Type","type":"String","value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p>The target can soar through the air, gaining a fly Speed equal to its Speed or 20 feet, whichever is greater.</p>\n<hr>\n<p><strong>Heightened (7th)</strong> The duration increases to 1 hour.</p>"},"duration":{"label":"Duration","type":"String","value":"5 minutes"},"level":{"label":"Spell Level","type":"Number","value":4},"location":{"value":"nUR46YzW01Eu49R1"},"materials":{"label":"Materials","type":"String","value":""},"prepared":{"label":"Prepared Spell","type":"String","value":""},"primarycheck":{"value":""},"range":{"label":"Range","type":"String","value":"touch"},"rarity":{"value":"common"},"save":{"basic":"","label":"Saving Throw","type":"String","value":""},"scaling":{"formula":"","mode":"none"},"school":{"label":"Spell School","type":"String","value":"trs"},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"source":{"label":"Source","type":"String","value":"https://2e.aonprd.com/Spells.aspx?ID=125"},"spellCategorie":{"value":"spell"},"spellCategory":{"value":""},"spellType":{"label":"Spell Type","type":"String","value":"utility"},"sustained":{"label":"Can be Sustained","type":"Boolean","value":false},"target":{"label":"Target","type":"String","value":"1 creature"},"time":{"label":"Casting Time","type":"String","value":"2"},"traditions":{"label":"Spell Traditions","type":"String","value":["arcane","occult","primal"]},"traits":{"la