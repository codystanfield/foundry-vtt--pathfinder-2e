":"Flint and Steel","sort":4900000,"type":"equipment"},{"_id":"f8DU0Ve40RvHxsOU","data":{"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":"a0qKUHz7rW9nPMNT"},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":""},"equipped":{"label":"Equipped","type":"Boolean","value":false},"equippedBulk":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"label":"Item Level","type":"Number","value":0},"maxHp":{"value":0},"negateBulk":{"value":"0"},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"label":"Price","type":"String","value":"4 sp (1 week)"},"quantity":{"label":"Quantity","type":"Number","value":7},"rarity":{"value":"common"},"rules":[],"source":{"label":"Source","type":"String","value":""},"stackGroup":{"value":"rations"},"traits":{"value":[]},"usage":{"value":"held-in-one-hand"},"weight":{"label":"Bulk","type":"Number","value":"L"}},"flags":{"_sheetTab":"description"},"img":"systems/pf2e/icons/equipment/adventuring-gear/rations.jpg","name":"Rations","sort":5300000,"type":"equipment"},{"_id":"kPRirKtMUoYYGGtP","data":{"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":"a0qKUHz7rW9nPMNT"},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p><span class=\"fontstyle0\">A torch sheds bright light in a 20‑foot radius (and dim light to the next 20 feet). It can be used as an improvised weapon that deals 1d4 bludgeoning damage plus 1 fire damage.</span> </p>"},"equipped":{"label":"Equipped","type":"Boolean","value":false},"equippedBulk":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"label":"Item Level","type":"Number","value":null},"maxHp":{"value":0},"negateBulk":{"value":"0"},"preciousMaterial":{"value":""},"preciousM