e":"Number","value":0},"maxHp":{"value":0},"negateBulk":{"value":"0"},"potencyRune":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"label":"Price","type":"String","value":"2 sp"},"property1":{"critDamage":"","critDamageType":"","critDice":null,"critDie":"","criticalConditionType":"","criticalConditionValue":null,"damageType":"","dice":null,"die":"","label":"Primary Property","strikeConditionType":"","strikeConditionValue":null,"type":"String","value":""},"property2":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Secondary Property","type":"String","value":""},"property3":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Tertiary Property","type":"String","value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"label":"Quantity","type":"Number","value":5},"range":{"label":"Weapon Range","type":"String","value":"10"},"rarity":{"value":"common"},"reload":{"label":"Reload","type":"String","value":""},"rules":[],"source":{"label":"Source","type":"String","value":""},"splashDamage":{"value":0},"stackGroup":{"value":""},"strikingRune":{"value":""},"traits":{"custom":"","label":"Weapon Traits","rarity":{"value":"common"},"type":"String","value":["agile","finesse","thrown-10","versatile-s"]},"usage":{"value":"held-in-one-hand"},"weaponType":{"label":"Weapon Category","type":"String","value":"simple"},"weight":{"label":"Bulk","type":"Number","value":"L"}},"flags":{"_sheetTab":"details"},"img":"systems/pf2e/icons/equipment/dagger.png","name":"Dagger","sort":300000,"type":"weapon"},{"_id":"oYqLMCi91W6VuJsH","data":{"containerId":{"value":""},"description":{"chat":"","unidentified":"","value":""},"featType":"","item":{"value":0},"mod":{"value":0},"proficient":{"value":1},"rarity":{"value":"common"},"rules":[],"source":{"value":""},"traits":{"value":[]},"usage":{"value":"held-in-one-hand"}},"flags":{},"img":"icons/svg/mystery-man.svg","name":"Sailing Lore","sort":100000,"type":"lore"},{"_id":"0Zblsq1lK15LKfZ6","data":{"actionCategory":{"value":""},"actionType":{"label":"Action Type","type":"String","value":"passive"},"actions":{"label":"Number of Actions","type":"String","value":""},"containerId":{"value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p>Your muscles are tightly honed. Your Speed increases by 5 feet.</p>"},"featType":{"label":"Feat Type","type":"String","value":"ancestry"},"level":{"label":"Feat Level","type":"String","value":"1"},"prerequisites":{"label":"Prerequisites","type":"String","value":""},"rarity":{"value":"common"},"rules":[],"source":{"label":"Source","type":"String","value":""},"traits":{"custom":"","label":"Traits","rarity":{"value":"common"},"type":"String","value":["elf"]},"usage":{"value":"held-in-one-hand"}},"flags":{"_sheetTab":"description"},"img":"systems/pf2e/icons/features/feats/feats.jpg","name":"Nimble Elf","sort":400000,"type":"feat"},{"_id":"rzg6BdkKiEChLq6L","data":{"actionCategory":{"value":""},"actionType":{"value":"passive"},"actions":{"value":""},"containerId":{"value":""},"description":{"chat":"","unidentified":"","value":"<p><strong>Prerequisites</strong> trained in Deception</p>\n<hr>\n<p>When you trick a foe, you can goad them into overextending their next attack.</p>\n<p>On a <a class=\"entity-link\" data-pack=\"pf2e.actionspf2e\" data-id=\"QNAVeNKtHA0EUw4X\" draggable=\"true\"><i class=\"fas fa-suitcase\"></i> Feint</a>, you can use the following success and critical success effects instead of any other effects you would gain when you Feint; if you do, other abilities that adjust the normal effects of your Feint no longer apply. You can choose whether to use the Goading Feint benefits or the normal benefits each time you Feint a given foe.</p>\n<p><strong>Critical Success</strong> The target takes a –2 circumstance penalty to <br>all attack rolls against you before the end of its next turn.</p>\n<p><strong>Success</strong> The target takes a –2 circumstance penalty to its next <br>attack roll against you before the end of its next turn.</p>"},"featType":{"value":"class"},"level":{"value":"1"},"prerequisites":{"value":"trained in Deception"},"rarity":{"value":"common"},"rules":[],"source":{"value":""},"traits":{"rarity":{"value":"common"},"value":["swashbuckler"]},"usage":{"value":""}},"flags":{},"img":"systems/pf2e/icons/features/feats/feats.jpg","name":"Goading Feint","sort":500000,"type":"feat"},{"_id":"h9A6Cb6ZzOD7PVpF","data":{"actionCategory":{"value":""},"actionType":{"label":"Action Type","type":"String","value":"passive"},"actions":{"label":"Number of Actions","type":"String","value":""},"containerId":{"value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p><strong>Prerequisites</strong> trained in Athletics</p>\n<hr>\n<p>You’ve learned to fight underwater. You are not flat-footed while in water, and you don’t take the usual penalties for using a bludgeoning or slashing melee weapon in water.</p>"},"featType":{"label":"Feat Type","type":"String","value":"skill"},"level":{"label":"Feat Level","type":"String","value":"1"},"prerequisites":{"label":"Prerequisites","type":"String","value":"trained in Athletics"},"rarity":{"value":"common"},"rules":[],"source":{"label":"Source","type":"String","value":""},"traits":{"custom":"","label":"Traits","rarity":{"value":"common"},"type":"String","value":["general","skill"]},"usage":{"value":"held-in-one-hand"}},"flags":{"_sheetTab":"description"},"img":"systems/pf2e/icons/features/feats/feats.jpg","name":"Underwater Marauder","sort":3550001,"type":"feat"},{"_id":"gSeDQkZQ65S8s73F","data":{"actionCategory":{"value":""},"actionType":{"value":"passive"},"actions":{"value":""},"containerId":{"value":""},"description":{"chat":"","unidentified":"","value":"<p>You gain an elegant finishing attack you can make when you have panache. The finisher trait is described below. You gain the @Compendium[pf2e.actionspf2e.K878asDgf1EF0B9S]{Confident Finisher} action.</p>\n<hr />\n<p><strong>Finisher:</strong> Finishers are spectacular finishing moves that&nbsp;use your panache. You can use a finisher only if you have&nbsp;panache, and you lose your panache immediately after&nbsp;performing a finisher. Once you use a finisher, you can&rsquo;t use&nbsp;actions that have the attack trait for the rest of your turn.</p>\n<p>Some actions with the finisher trait also grant an effect on&nbsp;a failure. Effects added on a failure don&rsquo;t apply on a critical&nbsp;failure. If your finisher action succeeds, you can still choose&nbsp;to apply the failure effect instead. For example, you might&nbsp;do this when an attack deals no damage due to resistance.&nbsp;</p>"},"featType":{"value":"classfeature"},"level":{"value":"1"},"prerequisites":{"value":""},"rarity":{"value":"common"},"rules":[],"source":{"value":""},"traits":{"custom":"","rarity":{"value":"common"},"selected":{"swashbuckler":"Swashbuckler"},"value":["swashbuckler"]},"usage":{"value":"held-in-one-hand"}},"flags":{},"img":"icons/svg/mystery-man.svg","name":"Confident Finisher","sort":700000,"type":"feat"},{"_id":"hxovDv0uksFiyTHG","data":{"actionCategory":{"value":""},"actionType":{"value":"passive"},"actions":{"value":""},"containerId":{"value":""},"description":{"chat":"","unidentified":"","value":"<p>You care as much about the way you accomplish something as whether you actually accomplish it in the first place. When you perform an action with particular bravado, you can leverage this moment of verve to perform spectacular, deadly maneuvers. This state of flair is called panache, and you are either in a state of panache or you are not.&nbsp;</p>\n<p>You gain panache by successfully performing the skill check associated with specific actions that have a bit of flair, including @Compendium[pf2e.actionspf2e.21WIfSu7Xd7uKqV8]{Tumble Through} and additional actions determined by your swashbuckler’s style (see below). At the GM’s discretion, after succeeding&nbsp;at a check to perform a particularly daring action, such as swinging on a chandelier or sliding down a drapery, you also gain panache if your result is high enough (typically&nbsp;the very hard DC for your level, but the GM can choose a different threshold).</p>\n<p>While you have panache, you gain a +5-foot status bonus to your Speeds and gain a +1 circumstance bonus to checks to Tumble Through or to take any actions that give you panache due to your style. The precise strike class feature (page 85) also causes you to deal extra precision damage while you have panache. Powerful finisher actions, including Confident Finisher (page 85), can be used only while you have panache and cause you to lose your panache.&nbsp;</p>\n<p>Normally, you gain and use panache only in combat encounters; when an encounter ends, you lose panache</p>"},"featType":{"value":"classfeature"},"level":{"value":"1"},"prerequisites":{"value":""},"rarity":{"value":"common"},"rules":[],"source":{"value":""},"traits":{"custom":"","rarity":{"value":"common"},"value":["swashbuckler"]},"usage":{"value":"held-in-one-hand"}},"flags":{},"img":"icons/svg/mystery-man.svg","name":"Panache","sort":800000,"type":"feat"},{"_id":"wUPWmwKFg2F6U82O","data":{"actionCategory":{"value":""},"actionType":{"value":"passive"},"actions":{"value":""},"containerId":{"value":""},"description":{"chat":"","unidentified":"","value":"<p>You @Compendium[pf2e.actionspf2e.VjxZFuUXrCU94MWR]{Strike} with flair. When you have panache and you Strike with an agile or finesse melee weapon or agile or finesse unarmed attack, you deal [[/r 2 #precision]] additional precision damage. If the strike is part of a finisher, the additional damage is [[/r 2d6 #precision]] precision damage instead.&nbsp;</p>\n<p>As your swashbuckler level increases, so does your additional damage for precise strike. Increase the amount of additional damage on a Strike and the number of additional dice on a finisher by one at 5th, 9th, 13th, and 17th levels.</p>"},"featType":{"value":"classfeature"},"level":{"value":"1"},"prerequisites":{"value":""},"rarity":{"value":"common"},"rules":[],"source":{"value":""},"traits":{"custom":"","rarity":{"value":"common"},"value":["swashbuckler"]},"usage":{"value":"held-in-one-hand"}},"flags":{},"img":"icons/svg/mystery-man.svg","name":"Precise Strike","sort":900000,"type":"feat"},{"_id":"EqiSQ2d4OnMf6RX0","data":{"actionCategory":{"value":""},"actionType":{"value":"passive"},"actions":{"value":""},"containerId":{"value":""},"description":{"chat":"","unidentified":"","value":"<p>You move carefully, feinting and creating false openings to lead your foes into inopportune attacks. You are trained in Deception. You gain @Compendium[pf2e.classfeatures.LzYi0OuOoypNb6jd]{Panache} during an encounter whenever you successfully @Compendium[pf2e.actionspf2e.QNAVeNKtHA0EUw4X]{Feint} or @Compendium[pf2e.actionspf2e.GkmbTGfg8KcgynOA]{Create a Diversion} against a foe.</p>"},"featType":{"value":"classfeature"},"level":{"value":"1"},"prerequisites":{"value":""},"rarity":{"value":"common"},"rules":[],"source":{"value":""},"traits":{"custom":"","rarity":{"value":"common"},"value":["swashbuckler"]},"usage":{"value":"held-in-one-hand"}},"flags":{},"img":"icons/svg/mystery-man.svg","name":"Fencer Style","sort":725000,"type":"feat"},{"_id":"hYHosLrYMTBUJ6ef","data":{"actionCategory":{"value":""},"actionType":{"value":"passive"},"actions":{"value":""},"containerId":{"value":""},"description":{"chat":"","unidentified":"","value":"<p>A creature with low-light vision can see in dim light as though it were bright light, so it ignores the @Compendium[pf2e.conditionspf2e.DmAIPqOBomZ7H95W]{Concealed} condition due to dim light.</p>"},"featType":{"value":"ancestryfeature"},"level":{"value":"1"},"prerequisites":{"value":""},"rarity":{"value":"common"},"rules":[],"source":{"value":""},"traits":{"custom":"","rarity":{"value":"common"},"value":["elf","gnome","half-Elf","half-Orc","leshy","shoony"]},"usage":{"value":"held-in-one-hand"}},"flags":{"_sheetTab":"description"},"img":"systems/pf2e/icons/features/ancestry/low-light-vision.jpg","name":"Low-Light Vision","sort":100001,"type":"feat"},{"_id":"PkvdpQkUXSbeY7DK","data":{"brokenThreshold":{"value":0},"bulkCapacity":{"value":"4"},"capacity":{"type":"weight","value":0,"weightless":false},"collapsed":{"value":false},"containerId":{"value":""},"currency":{"cp":0,"ep":0,"gp":0,"pp":0,"sp":0},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p>A backpack holds up to 4 Bulk of items. If you’re carrying or stowing the pack rather than wearing it on your back, its bulk is light instead of negligible.&nbsp;</p>"},"equipped":{"value":true},"equippedBulk":{"value":"0"},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"value":"0"},"maxHp":{"value":0},"negateBulk":{"value":"2"},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"label":"Price","type":"String","value":"1 sp"},"quantity":{"label":"Quantity","type":"Number","value":1},"rarity":{"value":"common"},"rules":[],"source":{"label":"Source","type":"String","value":""},"stackGroup":{"value":""},"traits":{"rarity":{"value":"common"},"value":[]},"usage":{"value":"held-in-one-hand"},"weight":{"label":"Bulk","type":"Number","value":"L"}},"flags":{"_sheetTab":"description"},"img":"systems/pf2e/icons/equipment/adventuring-gear/backpack.jpg","name":"Backpack","sort":1100000,"type":"backpack"},{"_id":"DvvM71OTv2uJ82r9","data":{"brokenThreshold":{"value":0},"bulkCapacity":{"value":"4L"},"capacity":{"type":"weight","value":0,"weightless":false},"collapsed":{"value":false},"containerId":{"value":""},"currency":{"cp":0,"ep":0,"gp":0,"pp":0,"sp":0},"description":{"chat":"","label":"Description","type":"String","unidenti