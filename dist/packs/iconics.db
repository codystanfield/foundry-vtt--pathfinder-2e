ue":""},"stackGroup":{"value":""},"traits":{"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"value":[]},"usage":{"value":"held-in-one-hand"},"weight":{"label":"Bulk","type":"Number","value":"-"}},"flags":{"_sheetTab":"description"},"img":"systems/pf2e/icons/equipment/adventuring-gear/soap.jpg","name":"Soap","sort":2100000,"type":"equipment"},{"_id":"ml1X5nDn802czgKW","data":{"armor":{"label":"AC Bonus","type":"Number","value":1},"armorType":{"label":"Armor Category","type":"String","value":"shield"},"brokenThreshold":{"value":3},"bulkCapacity":{"value":""},"check":{"label":"Check Penalty","type":"String","value":""},"collapsed":{"value":false},"containerId":{"value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p><span class=\"fontstyle0\">This very small shield is a favorite of duelists and quick, lightly armored warriors. It’s typically made of steel and strapped to your forearm. You can Raise a Shield with your buckler as long as you have that hand free or are holding a light object that’s not a weapon in that hand.</span></p>\n<table style=\"height: 32px;\" border=\"1\">\n<tbody>\n<tr style=\"height: 15px;\">\n<td style=\"height: 15px; width: 115px;\">Hardness</td>\n<td style=\"height: 15px; width: 115px;\">HP</td>\n<td style=\"height: 15px; width: 116px;\">BT</td>\n</tr>\n<tr style=\"height: 17px;\">\n<td style=\"height: 17px; width: 115px;\">3</td>\n<td style=\"height: 17px; width: 115px;\">6</td>\n<td style=\"height: 17px; width: 116px;\">3</td>\n</tr>\n</tbody>\n</table>"},"dex":{"label":"Dex Modifier Cap","type":"String","value":""},"equipped":{"label":"Equipped","type":"Boolean","value":true},"equippedBulk":{"value":""},"group":{"label":"Group","type":"String","value":""},"hardness":{"value":3},"hp":{"value":6},"identified":{"value":false},"invested":{"value":false},"level":{"label":"Item Level","type":"Number","value":0},"maxHp":{"value":6},"negateBulk":{"value":"0"},"potency":{"label":"Arm