ult. The attack ignores circumstance penalties to the attack roll and any flat check required due to the target being @Compendium[pf2e.conditionspf2e.DmAIPqOBomZ7H95W]{Concealed} or @Compendium[pf2e.conditionspf2e.iU0fEDdBp3rXpTMC]{Hidden}.</p>"},"duration":{"label":"Duration","type":"String","value":"until the end of your turn"},"level":{"label":"Spell Level","type":"Number","value":1},"location":{"value":"vL5RasoMgasgdDwB"},"materials":{"label":"Materials","type":"String","value":""},"prepared":{"label":"Prepared Spell","type":"String","value":""},"primarycheck":{"value":""},"range":{"label":"Range","type":"String","value":""},"rarity":{"value":"common"},"rules":[],"save":{"basic":"","label":"Saving Throw","type":"String","value":""},"scaling":{"formula":"","mode":"none"},"school":{"label":"Spell School","type":"String","value":"div"},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"source":{"label":"Source","type":"String","value":"https://2e.aonprd.com/Spells.aspx?ID=345"},"spellCategorie":{"value":"spell"},"spellCategory":{"value":""},"spellType":{"label":"Spell Type","type":"String","value":"utility"},"sustained":{"label":"Can be Sustained","type":"Boolean","value":false},"target":{"label":"Target","type":"String","value":""},"time":{"label":"Casting Time","type":"String","value":"1"},"traditions":{"label":"Spell Traditions","type":"String","value":["arcane","occult"]},"traits":{"label":"Spell Traits","rarity":{"value":"common"},"type":"String","value":["divination","fortune"]},"usage":{"value":"held-in-one-hand"}},"flags":{},"img":"systems/pf2e/icons/spells/true-strike.jpg","name":"True Strike","sort":12600001,"type":"spell"},{"_id":"ormXsRywpPiVy7FL","data":{"ability":{"label":"Spellcasting Ability","type":"String","value":""},"area":{"areaType":"","label":"Area","type":"String","value":null},"areasize":{"value":""},"components":{"label":"Spell Components","type":"String","value":"somatic, verbal"},"cost":{"label":"Cost","type":"String","value"