{"_id":"3Wb0N7iqmTn6e2Xc","data":{"activation":{"condition":"","cost":0,"type":""},"autoDestroy":{"_deprecated":true,"value":true},"autoUse":{"_deprecated":true,"value":true},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"charges":{"_deprecated":true,"max":"1","value":"1"},"collapsed":{"value":false},"consumableType":{"value":"potion"},"consume":{"_deprecated":true,"value":""},"containerId":{"value":""},"description":{"chat":"","unidentified":"","value":"<p><strong>Activate</strong>&nbsp;<span style=\"font-family: 'Pathfinder2eActions'; font-size: 1.8em; display: inline;\">1</span> Interact</p>\n<p>This slender vial appears to hold clean, clear water with a faintly fruity scent. Drinking a ration tonic magically nourishes you with the equivalent of a day's worth of food and water. The tonic has a subtle, pleasant taste, its particulars chosen when the potion is crafted.</p>"},"duration":{"units":"","value":null},"equipped":{"value":false},"equippedBulk":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"value":"1"},"maxHp":{"value":0},"negateBulk":{"value":"0"},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":"3 gp"},"quantity":{"value":"1"},"range":{"long":null,"units":"","value":null},"rarity":{"value":"common"},"source":{"value":""},"stackGroup":{"value":""},"target":{"type":"","units":"","value":null},"traits":{"custom":"","rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"value":["conjuration","consumable","magical","potion"]},"usage":{"value":"held-in-one-hand"},"uses":{"autoDestroy":true,"autoUse":true,"max":0,"per":null,"value":0},"weight":{"value":"L"}},"flags":{},"img":"systems/pf2e/icons/equipment/consumables/potions/ration-tonic.jpg","name":"Ration Tonic","permission":{"default":0},"type":"consumable"}
{"_id":"3Zv5hSXXtlaDatUv","data":{"MAP":{"value":""},"ability":{"label":"Offensive Ability","type":"String","value":"str"},"bonus":{"label":"Item Bonus","type":"String","value":0},"bonusDamage":{"value":0},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"damage":{"damageType":"piercing","dice":1,"die":"d10","label":"damage","type":"String","value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p>This pick has a longer handle and broader head than a regular pick. It is too heavy to wield in one hand.</p>"},"equipped":{"value":false},"equippedBulk":{"value":""},"group":{"label":"Weapon Group","type":"String","value":"pick"},"hands":{"label":"Hands","type":"String","value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"label":"Item Level","type":"Number","value":0},"maxHp":{"value":0},"negateBulk":{"value":"0"},"potencyRune":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"label":"Price","type":"String","value":"2 gp"},"property1":{"critDamage":"","critDamageType":"","critDice":null,"critDie":"","criticalConditionType":"","criticalConditionValue":null,"damageType":"","dice":null,"die":"","label":"Primary Property","strikeConditionType":"","strikeConditionValue":null,"type":"String","value":""},"property2":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Secondary Property","type":"String","value":""},"property3":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Tertiary Property","type":"String","value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"label":"Quantity","type":"Number","value":1},"range":{"label":"Weapon Range","type":"String","value":"melee"},"