{"_id":"FibwLZ12EIEwLGhw","data":{"MAP":{"value":""},"ability":{"label":"Offensive Ability","type":"String","value":"str"},"bonus":{"label":"Item Bonus","type":"String","value":0},"bonusDamage":{"value":0},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"damage":{"damageType":"bludgeoning","dice":1,"die":"d6","label":"damage","type":"String","value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p>This smaller version of the warhammer has a wooden or metal shaft ending in a metal head. Unlike its heavier cousin, it is light enough to throw.</p>"},"equipped":{"value":false},"equippedBulk":{"value":""},"group":{"label":"Weapon Group","type":"String","value":"hammer"},"hands":{"label":"Hands","type":"String","value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"label":"Item Level","type":"Number","value":0},"maxHp":{"value":0},"negateBulk":{"value":"0"},"potencyRune":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"label":"Price","type":"String","value":"3 sp"},"property1":{"critDamage":"","critDamageType":"","critDice":null,"critDie":"","criticalConditionType":"","criticalConditionValue":null,"damageType":"","dice":null,"die":"","label":"Primary Property","strikeConditionType":"","strikeConditionValue":null,"type":"String","value":""},"property2":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Secondary Property","type":"String","value":""},"property3":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Tertiary Property","type":"String","value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"label":"Quantity","type":"Number","value":1},"range":{"label":"Weap