{"_id":"e6arpbDxMlmogfee","data":{"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"denomination":{"value":"gp"},"description":{"chat":"","unidentified":"","value":""},"equipped":{"value":false},"equippedBulk":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"value":0},"maxHp":{"value":0},"negateBulk":{"value":"0"},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":0},"quantity":{"value":1},"rarity":{"value":"common"},"source":{"value":""},"stackGroup":{"value":""},"traits":{"value":[]},"usage":{"value":"held-in-one-hand"},"value":{"value":250},"weight":{"value":"-"}},"flags":{},"img":"systems/pf2e/icons/equipment/treasure/art-objects/greater-art-object/gold-and-aquamarine-diadem.jpg","name":"Gold and aquamarine diadem","permission":{"default":0},"type":"treasure"}
