{"_id":"iWcw1sFLAzByVLBP","data":{"MAP":{"value":""},"ability":{"label":"Offensive Ability","type":"String","value":"str"},"bonus":{"label":"Item Bonus","type":"String","value":0},"bonusDamage":{"value":0},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"damage":{"damageType":"piercing","dice":1,"die":"d4","label":"damage","type":"String","value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p>This parrying dagger features a robust guard to protect the wielder’s hand.</p>"},"equipped":{"value":false},"equippedBulk":{"value":""},"group":{"label":"Weapon Group","type":"String","value":"knife"},"hands":{"label":"Hands","type":"String","value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"label":"Item Level","type":"Number","value":0},"maxHp":{"value":0},"negateBulk":{"value":"0"},"potencyRune":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"label":"Price","type":"String","value":"5 sp"},"property1":{"critDamage":"","critDamageType":"","critDice":null,"critDie":"","criticalConditionType":"","criticalConditionValue":null,"damageType":"","dice":null,"die":"","label":"Primary Property","strikeConditionType":"","strikeConditionValue":null,"type":"String","value":""},"property2":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Secondary Property","type":"String","value":""},"property3":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Tertiary Property","type":"String","value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"label":"Quantity","type":"Number","value":1},"range":{"label":"Weapon Range","type":"String","value":"melee"},"rarity":{"value":"common"},"re