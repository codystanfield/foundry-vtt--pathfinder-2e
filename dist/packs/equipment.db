{"_id":"88pGCHV0uKMskTVO","data":{"activation":{"condition":"","cost":0,"type":""},"autoDestroy":{"_deprecated":true,"label":"Destroy on Empty","type":"Boolean","value":true},"autoUse":{"_deprecated":true,"label":"Consume on Use","type":"Boolean","value":true},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"charges":{"_deprecated":true,"label":"Charges","max":1,"type":"Number","value":1},"collapsed":{"value":false},"consumableType":{"label":"Consumable Type","type":"String","value":"potion"},"consume":{"_deprecated":true,"label":"Roll on Consume","type":"String","value":""},"containerId":{"value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p><strong>Activate</strong>&nbsp;<span style=\"font-family: 'Pathfinder2eActions'; font-size: 1.8em; display: inline;\">A</span> Interact</p>\n<p>After you drink this elixir, your sight becomes sharper in darkness. You gain darkvision for 10 minutes.</p>"},"duration":{"units":"","value":null},"equipped":{"value":false},"equippedBulk":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"label":"Item Level","type":"Number","value":2},"maxHp":{"value":0},"negateBulk":{"value":"0"},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"label":"Price","type":"String","value":"6 gp"},"quantity":{"label":"Quantity","type":"Number","value":1},"range":{"long":null,"units":"","value":null},"rarity":{"value":"common"},"rules":[],"source":{"label":"Source","type":"String","value":""},"stackGroup":{"value":""},"target":{"type":"","units":"","value":null},"traits":{"custom":"","rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"value":["alchemical","consumable","elixir"]},"usage":{"value":"held-in-one-hand"},"uses":{"autoDestroy":true,"autoUse":true,"max":0,"per":null,"value":0},"weight":{"label":"Bulk","type":"Number","value":"L"}},"flags":{},"img":"systems/pf2e/icons/equipment/al