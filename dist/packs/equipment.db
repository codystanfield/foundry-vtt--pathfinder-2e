{"_id":"RN3m5Hb5LPbdZGoO","data":{"MAP":{"value":""},"ability":{"value":"str"},"bonus":{"value":0},"bonusDamage":{"value":0},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"damage":{"damageType":"precision","dice":1,"die":"d8","value":""},"description":{"chat":"","unidentified":"","value":"<p>This asymmetrical bow, made of laminated bamboo, wood, and leather, stands 6 feet or more in height. Using a daikyu while mounted limits its firing range to your left side.</p>"},"equipped":{"value":false},"equippedBulk":{"value":""},"group":{"value":"bow"},"hands":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"value":"0"},"maxHp":{"value":0},"negateBulk":{"value":"0"},"potencyRune":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":"8 gp"},"property1":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","criticalConditionType":"","criticalConditionValue":null,"damageType":"","dice":0,"die":"","strikeConditionType":"","strikeConditionValue":null,"value":""},"property2":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","value":""},"property3":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"value":"1"},"range":{"value":"80"},"rarity":{"value":"common"},"reload":{"value":"-"},"rules":[],"source":{"value":""},"splashDamage":{"value":0},"stackGroup":{"value":""},"strikingRune":{"value":""},"traits":{"custom":"","rarity":{"value":"uncommon"},"value":[]},"usage":{"value":"held-in-one-hand"},"weaponType":{"value":"advanced"},"weight":{"value":"2"}},"flags":{},"img":"systems/pf2e/icons/equipment/weapons/daikyu.jpg","name":"Daikyu","permission":{"default":0},"typ