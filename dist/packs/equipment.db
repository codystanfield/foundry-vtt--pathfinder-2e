{"_id":"MPcM4Wt6KmWE2kGL","data":{"armor":{"label":"AC Bonus","type":"Number","value":2},"armorType":{"label":"Armor Category","type":"String","value":"light"},"brokenThreshold":{"value":null},"bulkCapacity":{"value":""},"check":{"label":"Check Penalty","type":"String","value":"-1"},"collapsed":{"value":false},"containerId":{"value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p><span class=\"fontstyle0\">Sometimes called a hauberk, this is a long shirt constructed of the same metal rings as chainmail. However, it is much lighter than chainmail and protects only the torso, upper arms, and upper legs of its wearer.</span> </p>"},"dex":{"label":"Dex Modifier Cap","type":"String","value":"3"},"equipped":{"label":"Equipped","type":"Boolean","value":false},"equippedBulk":{"value":"1"},"group":{"label":"Group","type":"String","value":"chain"},"hardness":{"value":null},"hp":{"value":null},"identified":{"value":false},"invested":{"value":false},"level":{"label":"Item Level","type":"Number","value":0},"maxHp":{"value":null},"negateBulk":{"value":"0"},"potency":{"label":"Armor Potency","type":"String"},"potencyRune":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"label":"Price","type":"String","value":"5 gp"},"property1":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Property Rune 1","type":"Number","value":""},"property2":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Property Rune 2","type":"Number","value":""},"property3":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Property Rune 3","type":"Number","value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"label":"Quantity","type":"Number","value":1},"rarity":{"value":"commo