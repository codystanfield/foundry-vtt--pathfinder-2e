{"_id":"xu3azdMCIa53Oe1f","data":{"MAP":{"value":""},"ability":{"value":"str"},"bonus":{"value":0},"bonusDamage":{"value":0},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"damage":{"damageType":"bludgeoning","dice":1,"die":"d8","value":""},"description":{"chat":"","unidentified":"","value":"<p><span>This weapon consists of a long chain connected to a heavy weight at one end. When a wielder swings the weight by the chain, it builds momentum and can serve as a deadly bludgeon with incredible reach.&nbsp;</span></p>"},"equipped":{"value":false},"equippedBulk":{"value":""},"group":{"value":"flail"},"hands":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"value":"0"},"maxHp":{"value":0},"negateBulk":{"value":"0"},"potencyRune":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":"3 gp"},"property1":{"critDamage":"","critDamageType":"","critDice":null,"critDie":"","criticalConditionType":"","criticalConditionValue":null,"damageType":"","dice":null,"die":"","strikeConditionType":"","strikeConditionValue":null,"value":""},"property2":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","value":""},"property3":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"value":"1"},"range":{"value":""},"rarity":{"value":"common"},"reload":{"value":""},"source":{"value":""},"splashDamage":{"value":0},"stackGroup":{"value":""},"strikingRune":{"value":""},"traits":{"custom":"","rarity":{"value":"common"},"value":["backswing","disarm","reach","trip"]},"usage":{"value":"held-in-one-hand"},"weaponType":{"value":"martial"},"weight":{"value":"2"}},"flags":{},"img":"systems/pf2e/icons/eq