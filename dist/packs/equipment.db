{"_id":"MYnh7w7EL3AcQT41","data":{"MAP":{"value":""},"ability":{"label":"Offensive Ability","type":"String","value":"str"},"bonus":{"label":"Item Bonus","type":"String","value":0},"bonusDamage":{"value":0},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"damage":{"damageType":"slashing","dice":1,"die":"d8","label":"damage","type":"String","value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p><span class=\"fontstyle0\">This favored weapon of the dwarves has a large, ornate head mounted on a thick handle. This powerful axe can be wielded with one hand or two.</span> </p>"},"equipped":{"value":false},"equippedBulk":{"value":""},"group":{"label":"Weapon Group","type":"String","value":"axe"},"hands":{"label":"Hands","type":"String","value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"label":"Item Level","type":"Number","value":0},"maxHp":{"value":0},"negateBulk":{"value":"0"},"potencyRune":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"label":"Price","type":"String","value":"3 gp"},"property1":{"critDamage":"","critDamageType":"","critDice":null,"critDie":"","criticalConditionType":"","criticalConditionValue":null,"damageType":"","dice":null,"die":"","label":"Primary Property","strikeConditionType":"","strikeConditionValue":null,"type":"String","value":""},"property2":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Secondary Property","type":"String","value":""},"property3":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Tertiary Property","type":"String","value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"label":"Quantity","type":"Number","value":