{"_id":"NufKswitJjxDCX8f","data":{"MAP":{"value":""},"ability":{"value":"str"},"bonus":{"value":0},"bonusDamage":{"value":0},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"damage":{"damageType":"slashing","dice":1,"die":"d8","value":""},"description":{"chat":"","unidentified":"","value":"<p><span>This 6-foot staff has a 2-foot-long, slightly curved, swordlike blade attached at one end. The long pole helps keep the wielder out of reach of swords and shorter weapons.&nbsp;</span></p>"},"equipped":{"value":false},"equippedBulk":{"value":""},"group":{"value":"polearm"},"hands":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"value":"0"},"maxHp":{"value":0},"negateBulk":{"value":"0"},"potencyRune":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":"3 gp"},"property1":{"critDamage":"","critDamageType":"","critDice":null,"critDie":"","criticalConditionType":"","criticalConditionValue":null,"damageType":"","dice":null,"die":"","strikeConditionType":"","strikeConditionValue":null,"value":""},"property2":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","value":""},"property3":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"value":"1"},"range":{"value":""},"rarity":{"value":"common"},"reload":{"value":""},"source":{"value":""},"splashDamage":{"value":null},"stackGroup":{"value":""},"strikingRune":{"value":""},"traits":{"custom":"","rarity":{"value":"uncommon"},"value":["deadly-d8","reach","versatile-p"]},"usage":{"value":"held-in-one-hand"},"weaponType":{"value":"martial"},"weight":{"value":"2"}},"flags":{},"img":"systems/pf2e/icons/equipment/weapons/naginata.jp