{"_id":"6AhDKX1dwRwFpQsU","data":{"armor":{"label":"AC Bonus","type":"Number","value":5},"armorType":{"label":"Armor Category","type":"String","value":"heavy"},"brokenThreshold":{"value":null},"bulkCapacity":{"value":""},"check":{"label":"Check Penalty","type":"String","value":"-3"},"collapsed":{"value":false},"containerId":{"value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p><span class=\"fontstyle0\">This type of armor is chain mail reinforced with flexible, interlocking metal plates, typically located on the wearer’s torso, upper arms, and legs. A suit of this armor comes with an undercoat of padded armor (see above) and a pair of gauntlets (page 285).</span> </p>"},"dex":{"label":"Dex Modifier Cap","type":"String","value":"1"},"equipped":{"label":"Equipped","type":"Boolean","value":false},"equippedBulk":{"value":"3"},"group":{"label":"Group","type":"String","value":"composite"},"hardness":{"value":null},"hp":{"value":null},"identified":{"value":false},"invested":{"value":false},"level":{"label":"Item Level","type":"Number","value":1},"maxHp":{"value":null},"negateBulk":{"value":"0"},"potency":{"label":"Armor Potency","type":"String"},"potencyRune":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"label":"Price","type":"String","value":"13 gp"},"property1":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Property Rune 1","type":"Number","value":""},"property2":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Property Rune 2","type":"Number","value":""},"property3":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","label":"Property Rune 3","type":"Number","value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"label":"Quantity","type":"Number","value":1},"rarity":{"value":"common"},"resiliencyRune":{"value":""},"resilient":{"label":"Resilient","type":"String"},"source":{"label":"Source","type":"String","value":""},"speed":{"label":"Speed Penalty","type":"String","value":"-10 ft"},"stackGroup":{"value":""},"strength":{"label":"Required Strength","type":"String","value":"16"},"traits":{"label":"Traits","rarity":{"value":"common"},"type":"String","value":""},"usage":{"value":"held-in-one-hand"},"weight":{"label":"Bulk","type":"Number","value":"4"}},"flags":{},"img":"systems/pf2e/icons/equipment/armor/splint-mail.jpg","name":"Splint Mail","permission":{"default":0},"type":"armor"}
{"_id":"6DCy7tEF1cxaIJMy","data":{"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"description":{"chat":"","label":"Description","type":"String","unidentified":"","value":"<p><span class=\"fontstyle0\">You can throw a grappling hook with a rope tied to it to make a climb easier. To anchor a grappling hook, make an attack roll with the secret trait against a DC depending on the target, typically at least DC 20. On a success, your hook has a firm hold, but on a critical failure, the hook seems like it will hold but actually falls when you’re partway up.</span> </p>"},"equipped":{"label":"Equipped","type":"Boolean","value":false},"equippedBulk":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"label":"Item Level","type":"Number","value":0},"maxHp":{"value":0},"negateBulk":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"label":"Price","type":"String","value":"1 sp"},"quantity":{"label":"Quantity","type":"Number","value":1},"rarity":{"value":"common"},"source":{"label":"Source","type":"String","value":""},"stackGroup":{"value":""},"traits":{"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"value":[]},"usage":{"value":"held-in-one-hand"},"weight":{"label":"Bulk","type":"Number","value":"L"}},"flags":{},"img":"systems/pf2e/icons/equipment/adventuring-gear/grapling-hook.jpg","name":"Grappling Hook","permission":{"default":0},"type":"equipment"}
{"_id":"6DmHDtIsGzH1s5JO","data":{"activation":{"condition":"","cost":0,"type":""},"autoDestroy":{"_deprecated":true,"value":true},"autoUse":{"_deprecated":true,"value":true},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"charges":{"_deprecated":true,"max":"1","value":"1"},"collapsed":{"value":false},"consumableType":{"value":"oil"},"consume":{"_deprecated":true,"value":""},"containerId":{"value":""},"description":{"chat":"","unidentified":"","value":"<p><span>You can spread this shimmering oil on an item of 1 Bulk or less to make it feel weightless. It has negligible Bulk for 1 hour.&nbsp;</span></p>"},"duration":{"units":"","value":null},"equipped":{"value":false},"equippedBulk":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"value":"2"},"maxHp":{"value":0},"negateBulk":{"value":"0"},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":"6 gp"},"quantity":{"value":"1"},"range":{"long":null,"units":"","value":null},"rarity":{"value":"common"},"source":{"value":""},"stackGroup":{"value":""},"target":{"type":"","units":"","value":null},"traits":{"custom":"","rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"value":["consumable","magical","oil","transmutation"]},"usage":{"value":"held-in-one-hand"},"uses":{"autoDestroy":true,"autoUse":true,"max":0,"per":null,"value":0},"weight":{"value":"L"}},"flags":{},"img":"systems/pf2e/icons/equipment/consumables/oils/oil-of-weightlessness.jpg","name":"Oil of Weightlessness","permission":{"default":0},"type":"consumable"}
{"_id":"6ELjUFY0sEJ7nZlZ","data":{"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"description":{"chat":"","unidentified":"","value":"<p><span>Magic wards deflect attacks. Increase the armor’s item bonus to AC by 3. The armor can be etched with three property runes.<br></span></p>\n<p><span>You can upgrade the <em>armor potency</em> rune already etched on a suit of armor to a stronger version, increasing the values of the existing rune to those of the new rune. You must have the formula of the stronger rune to do so, and the Price of the upgrade is the difference between the two runes’ Prices.</span></p>\n<p><span><strong>Craft Requirements</strong> You are an legendary in Crafting.</span></p>"},"equipped":{"value":false},"equippedBulk":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"value":"18"},"maxHp":{"value":0},"negateBulk":{"value":"0"},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":"20,560 gp"},"quantity":{"value":"1"},"rarity":{"value":"common"},"source":{"value":""},"stackGroup":{"value":""},"traits":{"custom":"","rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"value":["abjuration","magical"]},"usage":{"value":"held-in-one-hand"},"weight":{"value":"-"}},"flags":{},"img":"systems/pf2e/icons/equipment/runes/fundamental-armor-runes/armor-potency.jpg","name":"Armor Potency (+3)","permission":{"default":0},"type":"equipment"}
{"_id":"6HOQwVUQNplJsrhq","data":{"MAP":{"value":""},"ability":{"value":"str"},"bonus":{"value":0},"bonusDamage":{"value":0},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"damage":{"damageType":"piercing","dice":1,"die":"d6","value":""},"description":{"chat":"","unidentified":"","value":"<p>Hendrid Pratchett himself designed this custom skull-topped +1 striking exquisite sword cane and had it gilded in high-quality gold. Its first victim was the very smith whom Pratchett commissioned to craft the weapon, so the means of its construction are a secret known only to Pratchett. The magic of the <em>Reaper's Lancet</em> applies to both the blade and the sheath, making them each effectively a +1 striking weapon, but only if both are wielded by the same creature—if the two components are shared between creatures, only the blade retains the rune's magic.</p>\n<p><em>Reaper's Lancet </em>has a hidden reservoir inside the skull that automatically applies a new dose of poison to the blade after the previous dose was expended with a successful Strike. The reservoir must be filled manually (a process requiring 1 minute) and is large enough to hold only 3 doses at a time. Any sort of injury-delivered poison can be placed in it, though all doses must be of the same poison. The Price of the poison is not reflected in this weapon's listed Price.</p>\n<p><strong>Activate</strong>&nbsp;<span style=\"font-family: 'Pathfinder2eActions'; font-size: 1.8em; display: inline;\">R</span> Interact;&nbsp;</p>\n<p><strong>Trigger</strong> You damage a creature with Reaper's Lancet;</p>\n<p><strong>Effect</strong> You poison the creature you hit with the poison currently in the reservoir.&nbsp;</p>"},"equipped":{"value":false},"equippedBulk":{"value":""},"group":{"value":"sword"},"hands":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identified":{"value":false},"invested":{"value":false},"level":{"value":"5"},"maxHp":{"value":0},"negateBulk":{"value":"0"},"potencyRune":{"value":"1"},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":"200 gp"},"property1":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","criticalConditionType":"","criticalConditionValue":null,"damageType":"","dice":0,"die":"","strikeConditionType":"","strikeConditionValue":null,"value":""},"property2":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","value":""},"property3":{"critDamage":"","critDamageType":"","critDice":0,"critDie":"","damageType":"","dice":0,"die":"","value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"value":"1"},"range":{"value":""},"rarity":{"value":"common"},"reload":{"value":""},"rules":[],"source":{"value":""},"splashDamage":{"value":0},"stackGroup":{"value":""},"strikingRune":{"value":"striking"},"traits":{"custom":"","rarity":{"value":"unique"},"value":["agile","concealable","finesse","magical","necromancy","parry","poison","twin"]},"usage":{"value":"held-in-one-hand"},"weaponType":{"value":"martial"},"weight":{"value":"1"}},"flags":{},"img":"systems/pf2e/icons/equipment/weapons/specific-magic-weapons/reapers-lancet.jpg","name":"Reaper's Lancet","permission":{"default":0},"type":"weapon"}
